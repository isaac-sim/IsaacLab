    <child link="link_pitch_body"/>
    <axis xyz="0 0 -1"/>                              <!-- Rotation around negative Z-axis -->
    <limit lower="0" upper="1.047197551" effort="1000" velocity="2.6100"/>  <!-- 0-45 degree pitch -->
  </joint>

