        <mesh filename="package://meshes/link_up_down_body.STL"/>
      </geometry>
      <material name="">
        <color rgba="0.752941176470588 0.752941176470588 0.752941176470588 1"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://meshes/link_up_down_body.STL"/>
      </geometry>
    </collision>
  </link>

  <!-- Lift joint: Controls robot overall height, can rise 30cm -->
  <joint name="joint_lift_body" type="prismatic">
    <origin xyz="0 0 0.6485" rpy="0 0 0"/>  <!-- Starting from 34.85cm above base -->
    <parent link="base_link"/>
    <child link="link_up_down_body"/>
    <axis xyz="0 0 1"/>                      <!-- Movement along Z-axis -->
    <limit lower="0" upper="0.5" effort="1000" velocity="2.6100"/>  <!-- Lift range 0-30cm -->
  </joint>

  <!-- Body pitch link: Torso part that can tilt forward/backward -->
  <link name="link_pitch_body">
    <inertial>
      <origin xyz="0.193733558855111 -0.000484180179482319 -0.00124011422952511" rpy="0 0 0"/>
